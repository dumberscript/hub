local Tabs = {
    Automation = Window:AddTab({Title = "Automation", Icon = "bot"}),
    Trial = Window:AddTab({Title = "Trial", Icon = "target"}),
    Teleport = Window:AddTab({Title = "Teleport", Icon = "location"}),

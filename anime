                                                humanoid.WalkToPoint = character.HumanoidRootPart.Position
                                            end
                                        end

                                        if enemyInstance and enemyInstance.Parent then
                                            local args = {
                                                [1] = "General",
                                                [2] = "Pets",
                                                [3] = "Attack",
                                                [4] = currentPetId,
                                                [5] = enemyInstance

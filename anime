    StackInStackTwo,
    {
        __index = function()
            BanReason = "Stack Tampering"
        end,
        __tostring = function()
            BanReason = "Stack Tampering"
        end
    }
)

if type(getrawmetatable(StackInStackOne).__index) ~= "function" then
    BanReason = "Metatable Tampering"

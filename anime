for Index, Value in next, FunctionList do
    for index = 1, 197 do
        Value = coroutine.wrap(Value)
    end
    if Select(2, pcall(Value)) == "C stack overflow" then
        BanReason = "Function Hooking"
    end
end
JMPCount = JMPCount + 1
local LoadstringCheck = debug.info(2, "f")
if not LoadstringCheck then
end
